services:
  cefacapi:
    build:
      context: C:\Users\gedeo\source\repos\CefacAPI
      dockerfile: CefacAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CefacAPI
      target: base
    container_name: CefacAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: cefacapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/CefacAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\gedeo\source\repos\CefacAPI\CefacAPI:/app:rw
    - C:\Users\gedeo\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\gedeo\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\gedeo\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\gedeo\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\gedeo\source\repos\CefacAPI:/src:rw
version: '3.4'